doctype html
html
    head
        meta(charset="utf-8")
    body
        h1  List Page
        p
            a(href="/insert") INSERT DATA
        hr

        table(border=1)
            tr
                th Delete
                th Edit
                th ID
                th Name
                th Model Number
                th Series

            - for(var i = 0; i< results.length; i++){
                p
                    tr
                        td
                            a(href="/delete/"+results[i].id) Delete
                        td 
                            a(href="/edit/"+results[i].id) edit
                        td= results[i].id                
                        td= results[i].name                    
                        td= results[i].modelnumber                    
                        td= results[i].series
            - }

            
        
        